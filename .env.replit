# Replit Environment Variables
# These will be automatically set in Replit Secrets

# Database
USE_SQLITE=true
DATABASE_PATH=./data/kid-text-battle.db

# Server
PORT=3008
NODE_ENV=production

# Next.js
NEXT_TELEMETRY_DISABLED=1

# Security
JWT_SECRET=your-secret-key-here-change-in-replit-secrets
SESSION_SECRET=your-session-secret-here-change-in-replit-secrets

# Admin
ADMIN_USERNAME=admin
ADMIN_PASSWORD=1234